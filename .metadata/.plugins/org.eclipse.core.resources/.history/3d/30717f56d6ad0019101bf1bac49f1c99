<insert id="insertApi" parameterType="ApiDTO">
	INSERT INTO DOC_HOSP
	(
		yadmNm,
		clCd,
		clCdNm,
		sidoCd,
		sidoCdNm,
		sgguCd,
		sgguCdNm,
		emdongNm,
		postNo,
		addr,
		telno,
		hospUrl,
		estbDd,
		drTotCnt,
		gdrCnt,
		intnCnt,
		resdntCnt,
		sdrCnt,
		xPos,
		yPos,
		REGDATE,
		REGNO
	)
	VALUES
	(
		#{yadmNm},
	    #{clCd},
	    #{clCdNm},
	    #{sidoCd},
	    #{sidoCdNm},
	    #{sgguCd},
	    #{sgguCdNm},
	    #{emdongNm},
	    #{postNo},
	    #{addr},
	    #{telno},
	    #{hospUrl},
	    #{estbDd},
	    #{drTotCnt},
	    #{gdrCnt},
	    #{intnCnt},
	    #{resdntCnt},
	    #{sdrCnt},
	    #{XPos},
	    #{YPos},
	    NOW(),
	    #{hosp_no}
	)
</insert>

package poly.dto;

public class SidoDTO {
	private int sidoCd;
	private String sidoCdNm;
	public int getSidoCd() {
		return sidoCd;
	}
	public void setSidoCd(int sidoCd) {
		this.sidoCd = sidoCd;
	}
	public String getSidoCdNm() {
		return sidoCdNm;
	}
	public void setSidoCdNm(String sidoCdNm) {
		this.sidoCdNm = sidoCdNm;
	}
	
}
